public class samedata {

   public static void main(String[] args)
    {
        String columnist1 = "ramayana is about rama";
        String columnist2 = "mahabharatam";
        String columnist3 = "vishnu murthy";

        boolean equals1 = columnist1.equals(columnist2);
        boolean equals2 = columnist1.equals(columnist3);

        System.out.println("\"" + columnist1 + "\" equals \"" +
            columnist2 + "\": " + equals1);
        System.out.println("\"" + columnist1 + "\" equals \"" +
            columnist3 + "\": " + equals2);
    }
}
